prefix xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX oboInOwl: <http://www.geneontology.org/formats/oboInOwl#>
PREFIX replaced_by: <http://purl.obolibrary.org/obo/IAO_0100001>
PREFIX consider: <http://www.geneontology.org/formats/oboInOwl#consider>

SELECT ?cls ?cls_l ?cls_cm ?replCls ?replCls_l ?consCls ?consCls_l WHERE {
       ?cls a owl:Class  ;
	    owl:deprecated "true"^^xsd:boolean .
  OPTIONAL {?cls rdfs:label ?cls_l .}
  OPTIONAL {?cls rdfs:comment ?cls_cm .}
	OPTIONAL {?cls replaced_by: ?replCls .
    OPTIONAL {?replCls rdfs:label ?replCls_l .}
  }
	OPTIONAL {?cls consider: ?consCls .
    OPTIONAL {?consCls rdfs:label ?consCl_l .}
  }
}
ORDER BY ?cls
